from selenium import webdriver
from selenium.webdriver.common.by import By
import time
driver = webdriver.Chrome()
driver.get("https://fxposqaapp.azurewebsites.net/#/")
time.sleep(6)
driver.maximize_window()
time.sleep(3)
driver.find_element(By.XPATH,"//INPUT[@id='username']").click()
time.sleep(3)
driver.find_element(By.XPATH,"//INPUT[@id='username']").send_keys("qa1posadmin@idsnext.com")
time.sleep(3)
